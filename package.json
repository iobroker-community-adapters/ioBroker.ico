{
  "name": "iobroker.ico-cloud",
  "version": "0.0.5",
  "description": "ICO Pool sensor allow to monitor the state of the water in your pool and recommends actions to take.",
  "author": {
    "name": "Garfonso",
    "email": "garfonso@mobo.info"
  },
  "homepage": "https://github.com/iobroker-community-adapters/ioBroker.ico-cloud",
  "license": "MIT",
  "keywords": [
    "pool",
    "ico",
    "ondilo",
    "spa",
    "water"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iobroker-community-adapters/ioBroker.ico-cloud"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.1",
    "axios": "^0.21.1",
    "react-simple-oauth2-login": "^0.5.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^2.2.1",
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@iobroker/adapter-react": "1.6.15",
    "@iobroker/testing": "^2.5.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/mocha": "^8.2.3",
    "@types/node": "^14.17.5",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.14.11",
    "@types/react-dom": "^16.9.14",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "gulp": "^4.0.2",
    "mocha": "^9.1.1",
    "parcel-bundler": "^1.12.5",
    "proxyquire": "^2.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build:parcel": "parcel build admin/src/index.tsx -d admin/build",
    "build:ts": "tsc -p tsconfig.build.json",
    "build": "npm run build:ts && npm run build:parcel",
    "watch:parcel": "parcel admin/src/index.tsx -d admin/build",
    "watch:ts": "tsc -p tsconfig.build.json --watch",
    "watch": "npm run watch:ts",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/iobroker-community-adapters/ioBroker.ico-cloud/issues"
  },
  "readmeFilename": "README.md"
}
